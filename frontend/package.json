{
  "name": "aiapps",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "deploy": "expo export --platform web && firebase deploy",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "set NODE_ENV=test && playwright test",
    "test:ui": "set NODE_ENV=test && playwright test --ui",
    "test:headed": "set NODE_ENV=test && playwright test --headed",
    "test:debug": "set NODE_ENV=test && playwright test --debug"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@reduxjs/toolkit": "^2.9.0",
    "@types/lodash": "^4.17.20",
    "axios": "^1.11.0",
    "expo": "~53.0.22",
    "expo-firebase-core": "^6.0.0",
    "expo-status-bar": "~2.2.3",
    "firebase": "^12.2.1",
    "lodash": "^4.17.21",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-autocomplete-input": "^5.5.6",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-web": "^0.20.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@playwright/test": "^1.55.0",
    "@types/react": "~19.0.10",
    "@types/react-redux": "^7.1.34",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
