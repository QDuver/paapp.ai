# Firebase Configuration
# Get these values from Firebase Console > Project Settings

# Project Info
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_MESSAGING_SENDER_ID=your-sender-id
FIREBASE_STORAGE_BUCKET=your-project-id.firebasestorage.app
FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com

# Web Platform
FIREBASE_WEB_API_KEY=your-web-api-key
FIREBASE_WEB_APP_ID=your-web-app-id
FIREBASE_WEB_CLIENT_ID=your-web-client-id.apps.googleusercontent.com

# Android Platform
FIREBASE_ANDROID_API_KEY=your-android-api-key
FIREBASE_ANDROID_APP_ID=your-android-app-id
FIREBASE_ANDROID_CLIENT_ID=your-android-client-id.apps.googleusercontent.com

# iOS Platform
FIREBASE_IOS_API_KEY=your-ios-api-key
FIREBASE_IOS_APP_ID=your-ios-app-id
FIREBASE_IOS_CLIENT_ID=your-ios-client-id.apps.googleusercontent.com
FIREBASE_IOS_BUNDLE_ID=com.example.frontend

# Backend API
REACT_APP_API_URL=http://localhost:8000
